    <!-- JavaScript for Mobile Menu Toggle and Fullscreen -->
    <script>
        // 移动端菜单切换
        document.getElementById('menu-toggle').addEventListener('click', function() {
            const mobileMenu = document.getElementById('mobile-menu');
            mobileMenu.classList.toggle('hidden');
        });

        // 点击链接关闭移动端菜单
        document.querySelectorAll('#mobile-menu a').forEach(link => {
            link.addEventListener('click', function() {
                document.getElementById('mobile-menu').classList.add('hidden');
            });
        });

        // 全屏功能
        document.addEventListener('DOMContentLoaded', function() {
            const gameContainer = document.getElementById('game-container');
            const gameIframe = document.getElementById('game-iframe');
            const fullscreenBtn = document.getElementById('fullscreen-btn');
            const exitFullscreenBtn = document.getElementById('exit-fullscreen-btn');
            
            // 触摸设备检测
            const isTouchDevice = 'ontouchstart' in window || navigator.maxTouchPoints > 0;
            
            // 如果是触摸设备，显示全屏按钮
            if (isTouchDevice) {
                fullscreenBtn.style.opacity = '1';
            }
            
            // 移动端检测
            const isMobile = window.innerWidth <= 768;
            
            // 调整移动端游戏容器高度
            function adjustMobileGameHeight() {
                if (isMobile) {
                    const viewportWidth = window.innerWidth - 16; // 减去padding
                    const height = (viewportWidth * 9) / 16; // 16:9比例
                    gameContainer.style.height = height + 'px';
                }
            }
            
            // 页面加载和窗口大小改变时调整高度
            window.addEventListener('resize', adjustMobileGameHeight);
            adjustMobileGameHeight();
            
            // 进入全屏模式
            fullscreenBtn.addEventListener('click', function() {
                // 添加全屏类
                gameContainer.classList.add('fullscreen');
                
                // 禁止页面滚动
                document.body.style.overflow = 'hidden';
                
                // 滚动到游戏容器位置
                gameContainer.scrollIntoView();
                
                // 尝试使用原生全屏API (对iframe内容)
                try {
                    if (gameIframe.requestFullscreen) {
                        gameIframe.requestFullscreen();
                    } else if (gameIframe.webkitRequestFullscreen) { /* Safari */
                        gameIframe.webkitRequestFullscreen();
                    } else if (gameIframe.msRequestFullscreen) { /* IE11 */
                        gameIframe.msRequestFullscreen();
                    }
                } catch (e) {
                    console.log('Native fullscreen not supported, using CSS fullscreen');
                }
            });

            // 退出全屏模式
            exitFullscreenBtn.addEventListener('click', function() {
                // 移除全屏类
                gameContainer.classList.remove('fullscreen');
                
                // 允许页面滚动
                document.body.style.overflow = 'auto';
                
                // 尝试退出原生全屏
                try {
                    if (document.exitFullscreen) {
                        document.exitFullscreen();
                    } else if (document.webkitExitFullscreen) { /* Safari */
                        document.webkitExitFullscreen();
                    } else if (document.msExitFullscreen) { /* IE11 */
                        document.msExitFullscreen();
                    }
                } catch (e) {
                    console.log('Native fullscreen exit not supported');
                }
            });

            // 监听ESC键退出全屏
            document.addEventListener('keydown', function(e) {
                if (e.key === 'Escape' && gameContainer.classList.contains('fullscreen')) {
                    exitFullscreenBtn.click();
                }
            });
            
            // 监听原生全屏变化
            document.addEventListener('fullscreenchange', handleFullscreenChange);
            document.addEventListener('webkitfullscreenchange', handleFullscreenChange);
            document.addEventListener('mozfullscreenchange', handleFullscreenChange);
            document.addEventListener('MSFullscreenChange', handleFullscreenChange);
            
            function handleFullscreenChange() {
                if (!document.fullscreenElement && 
                    !document.webkitFullscreenElement && 
                    !document.mozFullScreenElement && 
                    !document.msFullscreenElement) {
                    // 如果退出了原生全屏，也退出我们的CSS全屏
                    gameContainer.classList.remove('fullscreen');
                    document.body.style.overflow = 'auto';
                }
            }
        });
    </script>

